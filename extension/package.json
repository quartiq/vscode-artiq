{
  "name": "artiq-log",
  "displayName": "artiq_log",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "panel": [
        {
          "id": "artiq",
          "title": "Artiq",
          "icon": "artiq.png"
        }
      ]
    },
    "views": {
      "artiq": [
        {
					"type": "webview",
					"id": "log",
					"name": "Log",
					"initialSize": 4,
          "icon": "log.png",
					"visibility": "visible"
				},
				{
					"type": "webview",
					"id": "schedule",
					"name": "Schedule",
					"initialSize": 1,
          "icon": "schedule.png",
					"visibility": "visible"
				}
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3",
    "ws": "^8.18.1"
  }
}
